{
  "name": "microsoft-copilot-porxy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "download-go-bingai-pass": "mkdir .tmp && wget https://github.com/Harry-zklcdc/go-bingai-pass/releases/latest/download/go-bingai-pass.wasm -O .tmp/go-bingai-pass.wasm",
    "deploy": "npm run build-worker && wrangler deploy",
    "dev": "wrangler dev",
    "build-ChallengeResponseBody-only": "rollup --config rollup.config.ChallengeResponseBody.mjs",
    "build-worker-only": "npm run build-ChallengeResponseBody-only && rollup --config rollup.config.workers.mjs",
    "build-page-only": "npm run build-ChallengeResponseBody-only && rollup --config rollup.config.pages.mjs",
    "build-worker": "npm run download-go-bingai-pass && npm run build-ChallengeResponseBody-only && npm run build-worker-only",
    "build-page": "npm run download-go-bingai-pass && npm run build-ChallengeResponseBody-only && npm run build-page-only",
    "iptest": "npx tsc ./iptest/iiptest.ts && node ./iptest/iiptest.js"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@cloudflare/vitest-pool-workers": "^0.1.0",
    "@cloudflare/workers-types": "^4.20240524.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-wasm": "^6.2.2",
    "rollup": "^4.18.0",
    "rollup-plugin-string": "^3.0.0",
    "typescript": "^5.0.4",
    "wrangler": "^3.0.0"
  }
}
